{
  "manifest_version": 1,
  "name": "StreamLine-Report-Renamer",
  "description": "Renames Files Downloaded from StreamLine",
  "version": "0.0.1",
  "minimum_chrome_version": "26.0.1428",

  "background": {
    "scripts": ["TGrenamer.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "activeTab",
	"downloads",
	"*://sol.worldpay.com/solweb/currencyreports/*"
  ]
}
